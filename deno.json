{
  "tasks": {
    "build": "rsbuild build",
    "lint:check": "oxlint -c oxlintrc.json --tsconfig tsconfig.json && prettier . --check",
    "lint:write": "oxlint -c oxlintrc.json --tsconfig tsconfig.json --fix && prettier . --write",
    "commitlint": "commitlint --edit $1",
    "lint-staged": "lint-staged"
  },
  "exports": {
    ".": "./src/index.ts"
  },
  "imports": {
    "@commitlint/cli": "npm:@commitlint/cli@^19.7.1",
    "@commitlint/config-conventional": "npm:@commitlint/config-conventional@^19.7.1",
    "@rsbuild/core": "npm:@rsbuild/core@^1.2.0",
    "@rsbuild/plugin-babel": "npm:@rsbuild/plugin-babel@^1.0.3",
    "@rspack/cli": "npm:@rspack/cli@^1.2.1",
    "@rspack/core": "npm:@rspack/core@^1.2.0",
    "@std/assert": "jsr:@std/assert@1",
    "husky": "npm:husky@^9.1.7",
    "lodash-es": "npm:lodash-es@^4.17.21",
    "prettier": "npm:prettier@^3.4.2"
  },
  "nodeModulesDir": "auto"
}
